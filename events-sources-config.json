  "filters": {
    "required_keywords": [
      "black", "african", "caribbean", "qtipoc", "queer", "trans",
      "lgbtq", "lgbt", "gay", "lesbian", "bisexual", "poc", "people of color"
    ],
    "exclude_keywords": [
      "corporate training", "diversity workshop for companies"
    ],
    "location_filter": {
      "countries": ["UK", "United Kingdom", "England", "Scotland", "Wales"],
      "cities": ["London", "Manchester", "Birmingham", "Bristol", "Leeds", "Glasgow", "Edinburgh"]
    }
  },

  "output_format": {
    "type": "event",
    "required_fields": ["title", "description", "source_url"],
    "optional_fields": ["event_date", "location", "organizer_name", "tags", "price", "image_url"],
    "batch_size": 10,
    "deduplication": {
      "enabled": true,
      "fields": ["title", "event_date", "location"]
    }
  },

  "error_handling": {
    "retry_attempts": 3,
    "retry_delay_seconds": 5,
    "skip_on_error": true,
    "notify_on_failure": true
  },

  "logging": {
    "level": "info",
    "log_scraping_stats": true,
    "log_webhook_responses": true
  }
}
